    "viem": "^2.9.25",
    "wait-on": "^7.2.0"
  },
  "devDependencies": {
    "@gbarros/noextra": "^0.1.3-beta",
    "@nomicfoundation/hardhat-ignition": "^0.15.0",
    "@nomicfoundation/hardhat-ignition-viem": "^0.15.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox-viem": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@nomicfoundation/hardhat-viem": "^2.0.0",
    "@types/chai": "^4.2.0",
    "@types/chai-as-promised": "^7.1.6",
    "@types/mocha": ">=9.1.0",
    "@types/node": "^20",
    "@types/wait-on": "^5.3.4",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "chai": "^4.2.0",
    "esbuild": "^0.19",
    "hardhat": "^2.14.0",
    "hardhat-gas-reporter": "^1.0.8",
    "npm-run-all": "^4",
    "openapi-typescript": "^6",
    "solidity-coverage": "^0.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^1.6.0"
  },
  "scripts": {
    "build": "run-s codegen compile",
    "codegen": "ts-node schema.ts",
    "compile": "tsc && esbuild ./src/index.ts --bundle --outfile=dist/index.js --platform=node --target=node20",
    "start": "ROLLUP_HTTP_SERVER_URL=\"http://127.0.0.1:5004\" ts-node src/index.ts",
    "dev": "nonodo -- yarn ts-node src/index.ts",
    "test": "vitest --coverage",
    "build:hardhat": "hardhat compile",
    "test:integration": "vitest test/integration",
    "clean:ignored": "rimraf artifacts cache coverage deployments/localhost dist generated-src src/types/*",
    "clean:hardhat": "hardhat clean",
    "clean": "run-s clean:hardhat clean:ignored"
  },
  "keywords": [
    "cartesi"
  ],
